QPushButton{
    background-color:rgb(44,158,247);
    color:white;
    border:0px solid;
    border-radius:4px;
}

QPushButton:hover{
    background-color:#92bffc;
    border:none;
}

QPushButton:pressed{
    background-color:#2857ad;
    padding-left:8px;
    padding-top:8px;
}


QTableWidget
{
    color:green;
    background:white;
    selection-color:white;
    border:1px solid grey;
}

QHeaderView{
    background:rgb(64,199,214);
    color:white;
}

QHeaderView::section:vertical{
    background:rgb(64,199,214);
    color:white;
}


QHeaderView::section:horizontal{
    background:rgb(64,199,214);
    color:white;
}

QScrollBar:horizontal
{
    height:40px;
    background:rgb(0,0,0,0%);
    margin:0px,0px,0px,0px;
    padding-top:1px;
    padding-bottom:1px;
    padding-left:40px;
    padding-right:40px;
    border:none;
}


QScrollBar::handle:horizontal,QScrollBar::handle:vertical
{
    height:40px;
    background:rgb(0,192,0,50%);
    border-radius:4px;
    min-width:20px;
    border:0px;
}


QScrollBar::add-line:horizontal
{
    height:40px;
    width:40px;
    border-image:url(:/res/image/right.png);
    subcontrol-position:right;
}

QScrollBar::sub-line:horizontal
{
    height:40px;
    width:40px;
    border-image:url(:/res/image/left.png);
    subcontrol-position:left;
}

QScrollBar:vertical {
    width:40px;
    background:rgb(0,0,0,0%);
    margin:0px,0px,0px,0px;
    padding-top:40px;
    padding-bottom:40px;
    border:0px solid red;
}

/* 底部控制块 */
QScrollBar::add-line:vertical {
    border-image:url(:/res/image/bottom.png);
    height:40px;
    width:40px;
    subcontrol-position:bottom;
    subcontrol-origin:margin;
}

QScrollBar::sub-line:vertical {
    border-image:url(:/res/image/top.png);
    height:40px;
    width:40px;
    subcontrol-position:top;
    subcontrol-origin:margin;
}



/* 滑块左边 */
QScrollBar::sub-page:horizontal,QScrollBar::sub-page:vertical {
    background:white;
}


/* 滑块右边 */
QScrollBar::add-page:horizontal,QScrollBar::add-page:vertical {
    background:white;
}


/*日期开始*/
QDateEdit
{
    color:green;
    background-color: white;
    spacing:40px;
    width:40px;
}

QDateEdit::drop-down {
    width:40px;
    spacing:0px;
}


#qt_calendar_calendarview {
    background: white;
}

#qt_calendar_navigationbar {
    background: rgba(215, 215, 215, 255);
}

QToolButton {
    icon-size: 30px, 30px;
    width: 60px;
    height: 30px;
}

QToolButton#qt_calendar_prevmonth {
    width: 50px;
    qproperty-icon: url(:/res/image/previous.png);
}

QToolButton#qt_calendar_nextmonth {
    width: 50px;
    qproperty-icon: url(:/res/image/next.png);
}

QToolButton#qt_calendar_monthbutton {
    padding-right: 30px;
    background:rgb(64,199,214);
    height:50px;
}

QToolButton#qt_calendar_yearbutton {
    background: rgb(127,233,127);
    width:80px;
    height:50px;
}

QSpinBox#qt_calendar_yearedit{
padding-right:20px;
border:1px solid green;
height:80px;
}

QSpinBox#qt_calendar_yearedit::up-button{
    width:40px;
    border-image:url(:/res/image/spintop.png);
}

QSpinBox#qt_calendar_yearedit::up-button:pressed{
    width:40px;
    border-image:url(:/res/image/spintop-pressed.png);
}

QSpinBox#qt_calendar_yearedit::down-button{
    width:40px;
    border-image:url(:/res/image/spinbottom.png);
}

QSpinBox#qt_calendar_yearedit::down-button:pressed{
    width:40px;
    border-image:url(:/res/image/spinbottom-pressed.png);
}

QToolButton#qt_calendar_monthbutton::menu-indicator{
    subcontrol-origin: padding;
    subcontrol-position: center right;
    right: 3px;
    width: 10px;
}
