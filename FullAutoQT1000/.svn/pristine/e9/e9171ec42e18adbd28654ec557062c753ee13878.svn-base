QPushButton{
    border: 1px solid gray;
    border-radius: 5px;
    background: rgb(0,163,110);
  /*background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(34,112,212), stop: 0.5 rgb(230,230,230),stop: 1.0 rgb(72,162,254),); */
        border-style:outset;
}

QPushButton:focus{
    padding: -2;
}

/**************************按钮图标********************************/
QPushButton#pb_Main_QC{
    color:rgb(255,255,255);
    background-color: rgb(25,25,49);
    qproperty-icon: url(:/res/image/QC);
    border: 0px solid gray;
    border-radius: 0px;
    border-style:outset;
}

QPushButton#pb_Main_Setting{
    color:rgb(255,255,255);
    background-color: rgb(25,25,49);
    qproperty-icon: url(:/res/image/Setting);
    border: 0px solid gray;
    border-radius: 0px;
    border-style:outset;
}

QPushButton#pb_Main_Test{
    color:rgb(255,255,255);
    background-color: rgb(25,25,49);
    qproperty-icon: url(:/res/image/Test);
    border: 0px solid gray;
    border-radius: 0px;
    border-style:outset;
}

QPushButton#pb_Main_Query{
    color:rgb(255,255,255);
    background-color: rgb(25,25,49);
    qproperty-icon: url(:/res/image/Query);
    border: 0px solid gray;
    border-radius: 0px;
    border-style:outset;
}

QPushButton#pb_Main_QC::checked,#pb_Main_Setting::checked,#pb_Main_Test::checked,#pb_Main_Query::checked{
    border-image:url(:);
    color:rgb(0,163,110);
    border: 0px solid gray;
    border-radius: 0px;
    border-style:outset;
}

QPushButton::pressed {
    /*border-image: url(:/resource/image/Hover.png) 0 4 0 4 repeat;*/
    background-color: rgb(178,226,210);
    border-top: 1px ;
    border-bottom: 0px ;
    border-right: 1px ;
    border-left: 0px ;
}

/*******************MainWindow**********************/
QMainWindow#MainWindow{
    background-color:rgb(25,25,49);
}

/*******************StackedWidget**********************/
QStackedWidget{
    background:rgb(1,207,156);
}

/*******************Dialog**********************/

QDialog{
    background:rgb(245,245,245);
}

QDialog QPushButton{
    height:30px;
        width:90px;
}

/*******************GroupBox**********************/
QGroupBox#groupBox_MainStatusBar {
    border-width:1px;
    border-style:solid;
    border-color:rgb(152,185,156);
    color:white;
    border-radius: 1px;
}









